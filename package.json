{
  "name": "opt-station",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "build": "npm-run-all --parallel format lint compile",
    "compile": "tsc --p tsconfig.build.json",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "validate": "npm-run-all --parallel lint test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TarekMostafaHr/optimal_station.git"
  },
  "bugs": {
    "url": "https://github.com/TarekMostafaHr/optimal_station.git#readme/issues"
  },
  "homepage": "https://github.com/TarekMostafaHr/optimal_station.git#readme",
  "keywords": [],
  "author": "Tarek Mostafa",
  "license": "MIT",
  "files": [
    "lib/**/**"
  ],
  "devDependencies": {
    "@types/jest": "24.0.18",
    "cross-env": "5.2.0",
    "ghooks": "2.0.4",
    "jest": "24.9.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "ts-jest": "24.0.2",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.5.3"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run validate"
    }
  }
